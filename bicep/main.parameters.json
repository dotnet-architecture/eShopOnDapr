{
    "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
    "contentVersion": "1.0.0.0",
    "parameters": {
        "location": {
            "value": "__location__"
        },
        "aadAdminGroupId": {
          "value": "__aadAdminGroupId__"
        },
        "vnetSettings": {
            "value": {
              "addressPrefixe": "10.0.0.0/8",
              "subnets": [
                {
                  "name": "snet-aks",
                  "addressPrefix": "10.0.0.0/16"
                },
                {
                  "name": "snet-jumpbox",
                  "addressPrefix": "10.1.0.0/24"
                },
                {
                  "name": "snet-private-endpoint",
                  "addressPrefix": "10.2.0.0/24"
                }                
              ]
            }
        },
        "aksAzureCniSettings": {
          "value": {
            "serviceCidr": "11.0.0.0/16",
            "dnsServiceIP": "11.0.0.10",
            "dockerBridgeCidr": "172.17.0.1/16"
          }
        },
        "ubuntuVersion": {
          "value": "18_04-lts-gen2"
        },
        "vmSize": {
          "value": "Standard_B2ms"
        },
        "adminUsername": {
          "value": "__adminUsername__"
        },
        "adminPassword": {
          "value": "__adminPassword__"
        },
        "adminSqlUsername": {
          "value": "__adminSqlUsername__",
          "type": "secureString"
        },
        "adminSqlPassword": {
          "value": "__adminSqlPassword__",
          "type": "secureString"
        }        
    }
}